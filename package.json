{
  "name": "react-sanfona",
  "version": "0.0.4",
  "description": "React accessible accordion component",
  "main": "./dist-modules/index.js",
  "scripts": {
    "bundle-demo": "webpack --config webpack.config.demo.js",
    "bundle-dist": "webpack --config webpack.config.dist.js",
    "bundle-modules": "trash dist-modules/* --force && babel src --loose --out-dir dist-modules",
    "demo": "webpack-dev-server --config webpack.config.demo.js",
    "lint": "eslint ./src",
    "test": "npm run lint && mocha -R spec --compilers jsx:test/compiler.js ./src/**/test.jsx"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daviferreira/react-sanfona.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "accordion",
    "accessibility",
    "aria",
    "widget"
  ],
  "author": "Davi Ferreira <hi@daviferreira.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daviferreira/react-sanfona/issues"
  },
  "homepage": "https://github.com/daviferreira/react-sanfona",
  "peerDependencies": {
    "react": "0.14.0",
    "react-dom": "0.14.0"
  },
  "dependencies": {
    "classnames": "2.1.5",
    "uuid": "2.0.1"
  },
  "devDependencies": {
    "autoprefixer-loader": "3.1.0",
    "babel": "5.8.29",
    "babel-core": "6.0.12",
    "babel-loader": "5.3.2",
    "css-loader": "0.19.0",
    "eslint": "1.6.0",
    "eslint-plugin-react": "3.5.1",
    "mocha": "2.3.3",
    "node-sass": "3.4.1",
    "normalize.css": "3.0.3",
    "react-addons-test-utils": "0.14.0",
    "sass-loader": "3.1.0",
    "sinon": "1.17.1",
    "skin-deep": "0.12.0",
    "style-loader": "0.12.4",
    "trash": "3.1.2",
    "unexpected": "10.0.2",
    "unexpected-sinon": "8.0.0",
    "webpack": "1.12.2",
    "webpack-dev-server": "1.12.0"
  }
}
